
{{!-- enctype="multipart/form-data" 様々のデータを取り扱えるという意味 --}}
<form action="/" method="post" enctype="multipart/form-data">
    <h3>画像をアップロードする</h3>
    <input type="file" name="imageFile">
    <input type="submit" />
</form>
<div>
    {{#each rows}}
    <p>下に保存された画像が出力されます。</p>
    <img src="{{this.imageName}}" alt="">
    <p>{{this.imageName}}</p>
    {{/each}}
</div>
